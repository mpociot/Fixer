{
    "name": "styleci/fixer",
    "description": "StyleCI Fixer Is A Code Style Report Builder",
    "keywords": ["Fixer", "CS", "StyleCI", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/StyleCI/PHP-CS-Fixer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "fabpot/php-cs-fixer": "~2.0.0",
        "styleci/cache": "~1.0",
        "styleci/config": "~2.0",
        "styleci/git": "~2.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "~3.0",
        "phpunit/phpunit": "^4.7.6"
    },
    "suggest": {
        "illuminate/support": "Enables Laravel support."
    },
    "autoload": {
        "psr-4": {
            "StyleCI\\Fixer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StyleCI\\Tests\\Fixer\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
