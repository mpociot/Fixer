{
    "name": "styleci/fixer",
    "description": "StyleCI Fixer Is A Code Style Report Builder",
    "keywords": ["Fixer", "CS", "StyleCI", "Alt Three"],
    "license": "MIT",
    "authors": [
        {
            "name": "James Brooks",
            "email": "james@alt-three.com"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Joseph Cohen",
            "email": "joe@alt-three.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "fabpot/php-cs-fixer": "~1.9.1",
        "styleci/config": "0.3.*",
        "styleci/git": "0.2.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "~3.0",
        "phpunit/phpunit": "^4.7.6"
    },
    "suggest": {
        "illuminate/support": "Enables Laravel support."
    },
    "autoload": {
        "psr-4": {
            "StyleCI\\Fixer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StyleCI\\Tests\\Fixer\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
